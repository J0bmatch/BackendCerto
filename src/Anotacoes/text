CREATE TABLE candidatos_test (
  CC_id INT AUTO_INCREMENT PRIMARY KEY,
  CC_cpf VARCHAR(11) UNIQUE NOT NULL,
  CC_email VARCHAR(45) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

